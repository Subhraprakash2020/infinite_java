package com.java.Day3;

class Student {
	private String name;
	private int age;

	public void setName(String newName) {
		name = newName;
	}

	public String getName() {
		return name;
	}

	public void setAge(int newAge) {
		if (newAge > 0) {
			age = newAge;
		}
	}

	public int getAge() {
		return age;
	}
}

public class Encapsulation {

}
