package com.java.Day1;

class Student {
	int rollno;
	String name;

	public Student(int rollno, String name) {
		this.rollno = rollno;
		this.name = name;
	}

	void display() {
		System.out.println(rollno + " " + name);
	}

}

public class ThisKeyWord {
	public static void main(String[] args) {
		Student s1 = new Student(111)
	}
}
